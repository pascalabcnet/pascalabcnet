# Recursive Factorial Calculation
def fact(n: int)-> int:
    if n == 0:
        return 1
    return n * fact(n - 1)


# Non-Recursive Fibonacci Number Calculation
def fib(n: int)-> int:
    if n <= 1:
        return n;
    f1: int = 0
    f2: int = 1
    for i in range(n - 1):
        t = f1 + f2
        f1 = f2
        f2 = t
    return f2