import random
b = [random.randint(0, 9) for i in range(20)]
b = [i for i in b if i % 2 == 0]
while True:
    i = int(input())
    if 0 <= i and i < len(b):
        print('your score is', b[i])
        break
    elif i >= len(b):
        print('try number less than', i)
    elif i < 0:
        print('try positive number like', 1)
    else:
        print('how is it even possible!?')
