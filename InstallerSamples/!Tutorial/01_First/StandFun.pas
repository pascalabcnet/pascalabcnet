// Стандартные функции

begin
  var x := ReadReal('Введите x:');
  Println('Квадрат', x, 'равен', Sqr(x));
  Println(x, 'в степени 5 равно', Power(x, 5));
  Println('Квадратный корень из', x, 'равен', Sqrt(x));
  Println('Модуль', x, 'равен', Abs(x));
  Println('Натуральный логарифм', x, 'равен', Ln(x));
  Println('Синус', x, 'равен', Sin(x));
  Println('Косинус', x, 'равен', Cos(x));
end.

