# индекс минимального элемента
def argmin(arr: list[int]) -> int:
    if len(arr) == 0:
        return -1
    res = 0
    for i in range(1, len(arr)):
        if arr[i] < arr[res]:
            res = i
    return res




print(argmin([1, 2, 3]))
print(argmin([3, 2, 1]))
